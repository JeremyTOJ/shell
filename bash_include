# environment variables
. ~/.shell/environment

# color definitions
. ~/.shell/colors

#aliases
. ~/.shell/aliases

# git functions
. ~/.shell/git_utils

# e environment manager
if [ -d ~/.e ]; then
	eval "$(~/.e/e.py init)"
fi

# prompt
. ~/.shell/prompt

# utility functions
. ~/.shell/utilities

# history control
. ~/.shell/history
